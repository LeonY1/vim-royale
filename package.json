{
  "name": "vim-royale",
  "version": "1.0.0",
  "description": "* Because Nano sucks. * Because Emacs sucks. * Because Rust sucks.",
  "main": "index.js",
  "scripts": {
      "c": "node ./node_modules/.bin/ts-node ./src/index.ts 2> out",
      "ci": "node --inspect ./node_modules/.bin/ts-node ./src/index.ts 2> out",
      "cib": "node --inspect-brk ./node_modules/.bin/ts-node ./src/index.ts 2> out",
      "s": "node --expose-gc ./node_modules/.bin/ts-node ./src/server/index.ts",
      "t": "node --expose-gc ./node_modules/.bin/jest",
      "tib": "node --inspect-brk --expose-gc ./node_modules/.bin/jest"
  },
